{
  "manifest_version": 2,
  "name": "keepTube",
  "description": "Keep youtube playing, always.",
  "version": "1.0",
  "browser_action": {
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
  ],
  "background": {
    "persistent": true,
    "scripts": ["main.js"]
  },
}